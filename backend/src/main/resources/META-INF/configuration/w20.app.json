{
  "/src/hub.w20.json": {
    "modules": {
      "toolbar": {
        "logoUrl": "http://seedstack.org/img/logos/seedstack-logo-round.svg",
        "brand": "SeedStack"
      }
    }
  },

  "w20-core": {
    "modules": {
      "application": {
        "id": "seedstack-hub",
        "home": "/hub/home",
        "blank": "/blank",
        "notFound": "/not-found",
        "redirectAfterRouteError": "/error"
      },
      "culture": {
        "available": [
          "en",
          "fr"
        ],
        "default": "en"
      },
      "env": {
        "type": "test"
      },
      "security": {
        "autoLogin": true
      },
      "utils": {},
      "hypermedia": {
        "api": {
          "home": "/"
        }
      }
    }
  },

  "w20-material": {}
}
